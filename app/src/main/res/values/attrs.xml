<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VoteView">
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>

        <attr name="spacing" format="dimension"/>
        <attr name="markedColor" format="color|reference"/>
        <attr name="markedColorDown" format="color|reference"/>
        <attr name="defaultColor" format="color|reference"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CommentSpacerView">
        <attr name="lineMargin" format="dimension"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="depth" format="integer"/>
    </declare-styleable>

    <declare-styleable name="AspectView">
        <attr name="aspect" format="float"/>
    </declare-styleable>
</resources>